  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.10",
    "cross-env": "^7.0.3",
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
